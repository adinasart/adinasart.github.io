				{alert("enter email ...");subscribeadina.email.focus();return false;}
				else
				if(subscribeadina.email.value.search("@") == -1 || subscribeadina.email.value.search("[.*]") == -1)
				  {alert("enter a valid e-mail address ...");subscribeadina.email.focus();return false;}
				else return true;                        
				}
				</script>
