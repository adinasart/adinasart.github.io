				function Validateform(forma)
				{
				if (recommandation.email.value == '')
				{alert("enter email ...");recommandation.email.focus();return false;}
				else
				if(recommandation.email.value.search("@") == -1 || recommandation.email.value.search("[.*]") == -1)
				  {alert("enter a valid e-mail address ...");recommandation.email.focus();return false;}
				else return true;                        
				}
				</script>
